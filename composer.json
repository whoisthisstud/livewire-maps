{
    "name": "esadewater/livewire-maps",
    "description": "Laravel Livewire-Maps",
    "keywords": [
        "esadewater",
        "laravel",
        "livewire",
        "livewire-component",
        "livewire-maps",
        "maps"
    ],
    "homepage": "https://github.com/esadewater/livewire-maps",
    "license": "MIT",
    "authors": [
        {
            "name": "Erik Sadewater",
            "email": "erik@sadewater.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "livewire/livewire": "^2.10",
        "spatie/laravel-package-tools": "^1.13.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-laravel": "^1.1",
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "ESadewater\\LivewireMaps\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ESadewater\\LivewireMaps\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ESadewater\\LivewireMaps\\LivewireMapsServiceProvider"
            ],
            "aliases": {
                "LivewireMaps": "ESadewater\\LivewireMaps\\Facades\\LivewireMaps"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
